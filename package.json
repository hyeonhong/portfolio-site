{
  "name": "portfolio-site",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "aws s3 sync out/. s3://hyeonhong-io.app",
    "copy-to-gh": "rm -rf ../hyeonhong.github.io/* && cp -a out/. ../hyeonhong.github.io/",
    "push-gh": "cd ../hyeonhong.github.io && git add . && git commit -m 'update' && git push",
    "deploy-to-gh": "yarn run build && yarn run copy-to-gh && yarn run push-gh"
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
